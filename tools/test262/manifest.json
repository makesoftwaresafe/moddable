{
	"build": {
		"BUILD": "$(MODDABLE)/build",
		"MODULES": "$(MODDABLE)/modules",
		"ESP32_SUBCLASS": "esp32s3"
	},
	"creation": {
		"static": 65536,
		"chunk": {
			"initial": 1536,
			"incremental": 512
		},
		"heap": {
			"initial": 512,
			"incremental": 64
		},
		"stack": 256,
		"keys": {
			"available": 16386,
			"name": 127,
			"symbol": 127
		},
		"parser": {
			"buffer": 9000,
			"table": 127
		},
		"main": "main"
	},
	"modules": {
		"*": [
			"./_262",
			"./harness",
			"./main",
			"$(MODULES)/base/instrumentation/modInstrumentation"
		]
	},
	"strip": [],
	"platforms": {
		"esp": {
			"error": "test262 is unsupported on esp8266"
		},
		"esp32": {
			"defines": {
				"XS_TEST": 1,
				"softreset": 1
			},
			"modules": {
				"*": [
					"$(MODULES)/base/timer/*",
					"$(MODULES)/base/timer/mc/*",
					"$(MODULES)/network/socket/lwip/modLwipSafe",
					"$(MODULES)/files/preference/*",
					"$(MODULES)/files/preference/esp32/*"
				]
			}
		},
		"esp32/esp32s3": {
			"build": {
				"ESP32_SUBCLASS": "esp32s3"
			},
			"defines": {
				"XS_TEST": 1,
				"softreset": 1
			},
			"modules": {
				"*": [
					"$(MODULES)/base/timer/*",
					"$(MODULES)/base/timer/mc/*",
					"$(MODULES)/network/socket/lwip/modLwipSafe",
					"$(MODULES)/files/preference/*",
					"$(MODULES)/files/preference/esp32/*"
				]
			}
		},
		"esp32/moddable_six_cdc": {
			"build": {
				"ESP32_SUBCLASS": "esp32s3",
				"USE_USB": "2",
				"SDKCONFIGPATH": "$(MODDABLE)/build/devices/esp32/targets/moddable_six_cdc/sdkconfig"
			},
			"creation": {
				"static": 0,
				"chunk": {
					"initial": 3145728,
					"incremental": 0 
				},
				"heap": {
					"initial": 196608,
					"incremental": 0 
				},
				"stack": 1024,
				"keys": {
					"available": 16386,
					"name": 127,
					"symbol": 127
				},
				"parser": {
					"buffer": 32768,
					"table": 127
				},
				"main": "main"
			},
			"defines": {
				"XS_TEST": 1,
				"softreset": 1
			},
			"modules": {
				"*": [
					"$(MODULES)/base/timer/*",
					"$(MODULES)/base/timer/mc/*",
					"$(MODULES)/network/socket/lwip/modLwipSafe",
					"$(MODULES)/files/preference/*",
					"$(MODULES)/files/preference/esp32/*"
				]
			}
		},
		"lin": {
			"build": {
				"SIMULATOR": "$(BUILD)/bin/lin/debug/mcsim"
			},
			"modules": {
				"*": [
					"$(BUILD)/simulators/modules/screen",
					"$(MODULES)/base/timer/*",
					"$(MODULES)/base/timer/lin/*"
				]
			}
		},
		"mac": {
			"build": {
				"SIMULATOR": "$(BUILD)/bin/mac/debug/mcsim.app"
			},
			"modules": {
				"*": [
					"$(BUILD)/simulators/modules/screen",
					"$(MODULES)/base/time/*",
					"$(MODULES)/base/time/mac/*",
					"$(MODULES)/base/timer/*",
					"$(MODULES)/base/timer/mac/*"
				]
			}
		},
		"win": {
			"build": {
				"SIMULATOR": "$(BUILD)/bin/win/debug/mcsim.exe"
			},
			"modules": {
				"*": [
					"$(BUILD)/simulators/modules/screen",
					"$(MODULES)/base/time/*",
					"$(MODULES)/base/time/win/*",
					"$(MODULES)/base/timer/*",
					"$(MODULES)/base/timer/win/*"
				]
			}
		}
	}
}
